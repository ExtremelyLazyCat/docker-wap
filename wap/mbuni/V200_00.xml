			<rdf:li>URL</rdf:li>
			<rdf:li>WMLBrowser</rdf:li>
			<rdf:li>Dialogs</rdf:li>
		</rdf:Bag>
        </prf:WmlScriptLibraries>  

<!--*********** WTAI LIBRARY ************-->
        <prf:WtaiLibraries>
		<rdf:Bag>
			<rdf:li>WTA.Public.makeCall</rdf:li>
			<rdf:li>WTA.Public.sendDTMF</rdf:li>
		</rdf:Bag>
        </prf:WtaiLibraries>  
         
<!--******************************-->
	</rdf:Description>
</prf:component>

